<launch>

<rosparam file="$(find agribot_v4_description)/launch/controller_updated.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="agribot_v4" args="front_right_drive_position_controller front_left_drive_position_controller back_left_drive_position_controller back_right_drive_position_controller arm_with_eef_controller steering_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/agribot_v4/joint_states"/>
</node>

</launch>