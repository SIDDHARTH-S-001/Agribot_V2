<launch>
  <param name="robot_description" command="$(find xacro)/xacro $(find agribot_testing_1_description)/urdf/agribot_testing_1.xacro"/>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -model agribot_testing_1"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <rosparam file="$(find agribot_testing_1_description)/param/skid_steer_controller.yaml" command="load"/>
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" args="agribot_testing_1_controller"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
       <!-- <remap from="/joint_states" to="/agribot_testing_1/joint_states"/> -->
    </node>

</launch>
